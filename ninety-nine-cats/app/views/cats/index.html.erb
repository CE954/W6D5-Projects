<h1>All the cats!</h1>


<% @cats.each do |cat| %>
    <ul>
        <li>Name: <%= cat.name %> </li>
        <li>Birth date: <%= cat.birth_date %> </li>
        <li>Color: <%= cat.color %> </li>
        <li>Sex: <%= cat.sex %> </li>
        <li><a href="<%= cat_url(cat.id) %>">Profile</a></li>
    </ul>
<% end %>